{
  "version": "1.0",
  "policy_id": "test_compute_divide",
  "description": "Field validation test policy: Tests division operator in compute expressions. Releases when effective_liquidity >= remaining_amount / 2.0 (half payment threshold).",
  "payment_tree": {
    "type": "condition",
    "node_id": "TEST-N1-Divide",
    "description": "Does agent have half the payment amount?",
    "condition": {
      "op": ">=",
      "left": {
        "field": "effective_liquidity"
      },
      "right": {
        "compute": {
          "op": "/",
          "left": {
            "field": "remaining_amount"
          },
          "right": {
            "value": 2.0
          }
        }
      }
    },
    "on_true": {
      "type": "action",
      "node_id": "TEST-A1-DivideRelease",
      "action": "Release"
    },
    "on_false": {
      "type": "action",
      "node_id": "TEST-A2-Hold",
      "action": "Hold",
      "parameters": {
        "reason": {
          "value": "BelowDivideThreshold"
        }
      }
    }
  },
  "strategic_collateral_tree": {
    "type": "action",
    "node_id": "TEST-SC1-NoAction",
    "action": "HoldCollateral"
  },
  "end_of_tick_collateral_tree": {
    "type": "action",
    "node_id": "TEST-EC1-NoAction",
    "action": "HoldCollateral"
  }
}
